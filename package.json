{
  "name": "express-ts-starter-kit",
  "version": "1.0.0",
  "description": "NodeJS Express Typescript Starter Kit",
  "scripts": {
    "start": "node dist/index.js",
    "build": "ts-node build.ts && tsc --project tsconfig-build.json",
    "dev": "nodemon",
    "dev:debug": "nodemon --config nodemon-debug.json",
    "lint": "eslint --config .eslintrc.json --ext .ts --ext .js server/",
    "compile": "tsc --project tsconfig-build.json",
    "test": "mocha -r ts-node/register server/**/*.specs.ts --exit",
    "test:debug": "mocha --inspect-brk -r ts-node/register server/**/*.specs.ts --exit",
    "husky": "npm run lint && precise-commits",
    "db:create": "sequelize db:create",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run husky"
    }
  },
  "dependencies": {
    "@types/lodash": "^4.14.184",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.4.7",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "axios": "^0.27.2",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.0",
    "chai": "^4.3.6",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv-safe": "^8.2.0",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.1",
    "fs": "0.0.1-security",
    "helmet": "^5.1.1",
    "lodash": "^4.17.21",
    "mocha": "^10.0.0",
    "moment": "^2.29.4",
    "pino": "^8.4.1",
    "prettier": "^2.7.1",
    "sequelize": "^6.21.4",
    "sequelize-cli": "^6.4.1",
    "shelljs": "^0.8.5",
    "supertest": "^6.2.4",
    "swagger-ui-express": "^4.5.0",
    "typescipt": "^1.0.0",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/chai": "^4.3.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/dotenv-safe": "^8.1.2",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.11.50",
    "@types/pino": "^7.0.4",
    "@types/shelljs": "^0.8.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.19",
    "pino-pretty": "^9.0.1",
    "precise-commits": "^1.0.2",
    "ts-node": "^10.9.1",
    "tslint": "^5.20.1",
    "typescript": "^4.7.4"
  }
}
